			<control type="group">
				<posx>64</posx>
				<visible>!Window.IsVisible(1112)</visible>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>280</width>
					<height>20</height>
					<font>font20_title</font>
					<aligny>center</aligny>
					<label>$INFO[Player.Time(hh:mm:ss)]</label>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>40</posy>
					<width>552</width>
					<height>32</height>
					<font>font20_title</font>
					<align>center</align>
					<aligny>center</aligny>
					<label>$VAR[SeekLabel]</label>
				</control>
				<control type="label">
					<posx>1400</posx>
					<posy>0</posy>
