<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="EventListView">
		<!--all content-->
		<control type="group">
			<include condition="String.IsEqual(Skin.AspectRatio,4:3)">TallFatListContainer</include>
			<include condition="!String.IsEqual(Skin.AspectRatio,4:3)">ShortListContainer</include>
			<centerleft>50%</centerleft>
			<width>1501</width>
			<visible>Control.IsVisible(570)</visible>
			<include>VisibleFadeEffect</include>
			<!--content list-->
			<control type="list" id="570">
				<top>5</top>
				<width>1004</width>
				<include condition="String.IsEqual(Skin.AspectRatio,4:3)">TallFatListSlide</include>
				<include condition="!String.IsEqual(Skin.AspectRatio,4:3)">ShortFatListSlide</include>
				<onleft>2</onleft>
				<onright>60</onright>
				<onup>570</onup>
				<ondown>570</ondown>
				<viewtype label="535">list</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<itemlayout height="100" width="1004">
					<!--background-->
					<control type="image">
						<left>4</left>
						<top>0</top>
						<width>1000</width>
						<height>90</height>
						<texture colordiffuse="button.nf">backgrounds/white.png</texture>
					</control>
					<!--event name-->
					<control type="label">
						<left>19</left>
						<top>0</top>
						<width>800</width>
						<height>50</height>
						<font>label</font>
						<textcolor>white</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!--date time-->
					<control type="textbox">
						<left>835</left>
						<top>0</top>
						<width>150</width>
						<height>90</height>
						<font>secondary.info</font>
						<textcolor>white</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<!--event info-->
					<control type="textbox">
						<left>19</left>
						<top>35</top>
						<width>800</width>
						<height>50</height>
						<font>secondary.info.small</font>
						<textcolor>white</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(Event.Description)]</label>
					</control>
				</itemlayout>
				<focusedlayout height="100" width="1004">
					<!--background-->
					<control type="image">
						<left>4</left>
						<top>0</top>
						<width>1000</width>
						<height>90</height>
						<texture colordiffuse="tertiary">backgrounds/white.png</texture>
						<animation effect="fade" start="100" end="75" time="200" condition="Control.HasFocus(60)">Conditional</animation>
					</control>
					<!--background strip-->
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>4</width>
						<height>90</height>
						<texture colordiffuse="$VAR[Colour.500]">backgrounds/white.png</texture>
						<animation effect="fade" start="100" end="75" time="200" condition="Control.HasFocus(60)">Conditional</animation>
					</control>
					<!--event name-->
					<control type="label">
						<left>19</left>
						<top>0</top>
						<width>800</width>
						<height>50</height>
						<font>label</font>
						<textcolor>text.primary</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!--date time-->
					<control type="textbox">
						<left>835</left>
						<top>0</top>
						<width>150</width>
						<height>90</height>
						<font>secondary.info</font>
						<textcolor>text.primary</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<!--event info-->
					<control type="textbox">
						<left>19</left>
						<top>35</top>
						<width>800</width>
						<height>50</height>
						<font>secondary.info.small</font>
						<textcolor>text.primary</textcolor>
						<selectedcolor>$VAR[Colour.500]</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(Event.Description)]</label>
					</control>
				</focusedlayout>
			</control>
			<!--thumb (fallback)-->
			<control type="image">
				<left>1021</left>
				<centertop>50%</centertop>
				<width>480</width>
				<height>710</height>
				<aspectratio aligny="center">keep</aspectratio>
				<texture colordiffuse="$VAR[Colour.500]" background="true">$VAR[PosterThumb]</texture>
				<bordertexture>backgrounds/poster shadow fb.png</bordertexture>
				<bordersize>10</bordersize>
				<visible>String.Contains(ListItem.Icon,default)</visible>
			</control>
			<!--thumb (no fallback)-->
			<control type="image">
				<left>1021</left>
				<centertop>50%</centertop>
				<width>480</width>
				<height>710</height>
				<aspectratio aligny="center">keep</aspectratio>
				<texture background="true">$VAR[PosterThumb]</texture>
				<bordertexture>backgrounds/poster shadow.png</bordertexture>
				<bordersize>10</bordersize>
				<visible>!String.Contains(ListItem.Icon,default)</visible>
			</control>
			<!--scrollbar-->
			<control type="scrollbar" id="60">
				<left>1014</left>
				<top>5</top>
				<bottom>5</bottom>
				<onleft>570</onleft>
				<onright>2</onright>
				<orientation>vertical</orientation>
			</control>
		</control>
	</include>
