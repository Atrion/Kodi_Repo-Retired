<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">9</defaultcontrol>
	<controls>
		<!--all content-->
		<control type="group">
			<centerleft>50%</centerleft>
			<width>1210</width>
			<animation effect="slide" start="1925,0" end="0,0" time="400" tween="quadratic" easing="out">WindowOpen</animation>
			<animation effect="slide" start="0,0" end="1925,0" time="400" tween="quadratic" easing="out">WindowClose</animation>
			<animation effect="slide" start="0,0" end="-35,0" time="300" condition="Window.IsActive(ShutdownMenu) + !String.IsEqual(Skin.AspectRatio,21:9)">Conditional</animation>
			<!--title bars and backgrounds-->
			<include content="DynamicDialogBackground">
				<param name="dialog-width" value="1210" />
				<param name="glow-width" value="1290" />
				<param name="sidebar-visible" value="true" />
				<param name="sidebar-width" value="280" />
				<param name="buttonbar-visible" value="true" />
			</include>
			<!--close window button-->
			<control type="button">
				<left>5</left>
				<top>0</top>
				<width>50</width>
				<height>25</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>3</onleft>
				<onright>3</onright>
				<onup>3</onup>
				<ondown>3</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<!--main content-->
			<control type="group">
				<!--title-->
				<control type="label" id="20">
					<left>45</left>
					<top>25</top>
					<width>1120</width>
					<height>60</height>
					<font>info.title</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>white</textcolor>
					<label>-</label>
				</control>
				<control type="grouplist" id="9">
					<description>button area</description>
					<left>10</left>
					<top>120</top>
					<bottom>120</bottom>
					<width>330</width>
					<itemgap>5</itemgap>
					<pagecontrol>31</pagecontrol>
					<onleft>31</onleft>
					<onright>2</onright>
					<onup>9</onup>
					<ondown>9</ondown>
				</control>
				<!--scrollbar-->
				<control type="scrollbar" id="31">
					<left>-12</left>
					<top>120</top>
					<height>880</height>
					<onleft>30</onleft>
					<onright>9</onright>
				</control>
				<control type="grouplist" id="2">
					<description>control area</description>
					<left>290</left>
					<top>120</top>
					<bottom>120</bottom>
					<width>910</width>
					<itemgap>5</itemgap>
					<pagecontrol>30</pagecontrol>
					<onup>2</onup>
					<ondown>9001</ondown>
					<onleft>9</onleft>
					<onright>30</onright>
				</control>
				<!--scrollbar-->
				<control type="scrollbar" id="30">
					<left>1215</left>
					<top>120</top>
					<height>880</height>
					<onleft>2</onleft>
					<onright>31</onright>
				</control>
				<!--category buttons-->
				<control type="button" id="13">
					<width>260</width>
					<texturefocus colordiffuse="white">backgrounds/white.png</texturefocus>
					<textcolor>white</textcolor>
					<focusedcolor>$VAR[Colour.900]</focusedcolor>
					<include>ButtonCommonValues</include>
				</control>
				<!--button-->
				<control type="button" id="3">
					<left>0</left>
					<top>0</top>
					<invalidcolor>invalid</invalidcolor>
				</control>
				<!--radio button-->
				<control type="radiobutton" id="4">
					<left>0</left>
					<top>0</top>
				</control>
				<!--spin control-->
				<control type="spincontrolex" id="5">
					<left>0</left>
					<top>0</top>
					<reverse>yes</reverse>
				</control>
				<!--slider control-->
				<control type="sliderex" id="8">
					<left>0</left>
					<top>0</top>
				</control>
				<control type="label" id="7">
					<height>60</height>
					<label>-</label>
					<font>button.bold</font>
					<textcolor>$VAR[Colour.500]</textcolor>
					<textoffsetx>5</textoffsetx>
					<aligny>center</aligny>
				</control>
				<control type="image" id="6">
					<description>Default Seperator</description>
					<height>0</height>
					<texture>-</texture>
					<visible>false</visible>
				</control>
				<!--bottom buttons-->
				<control type="grouplist" id="9001">
					<left>117</left>
					<top>65r</top>
					<width>1000</width>
					<height>60</height>
					<align>justify</align>
					<orientation>horizontal</orientation>
					<onleft>9001</onleft>
					<onright>9001</onright>
					<onup>2</onup>
					<ondown>2</ondown>
					<!--ok button-->
					<include content="AnimatedCircleButtonGroup">
						<param name="button-id" value="10" />
						<param name="button-width" value="130" />
						<param name="button-texture" value="yesbutton" />
						<param name="button-label" value="186" />
						<param name="button-onup" value="2" />
						<param name="button-ondown" value="2" />
						<param name="button-onleft" value="12" />
						<param name="button-onright" value="11" />
					</include>
					<!--cancel button-->
					<include content="AnimatedCircleButtonGroup">
						<param name="button-id" value="11" />
						<param name="button-width" value="180" />
						<param name="button-texture" value="nobutton" />
						<param name="button-label" value="222" />
						<param name="button-onup" value="2" />
						<param name="button-ondown" value="2" />
						<param name="button-onleft" value="10" />
						<param name="button-onright" value="12" />
					</include>
					<!--default button-->
					<include content="AnimatedCircleButtonGroup">
						<param name="button-id" value="12" />
						<param name="button-width" value="180" />
						<param name="button-texture" value="refreshbutton" />
						<param name="button-label" value="409" />
						<param name="button-onup" value="2" />
						<param name="button-ondown" value="2" />
						<param name="button-onleft" value="11" />
						<param name="button-onright" value="10" />
					</include>
				</control>
			</control>
		</control>
	</controls>
</window>